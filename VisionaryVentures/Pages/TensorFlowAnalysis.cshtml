@page
@model VisionaryVentures.Pages.TensorFlowAnalysisModel

@{
    ViewData["Title"] = "TensorFlow Analysis";
}

<h2>TensorFlow Analysis: @Model.FileName</h2>

@if (Model.Headers != null && Model.Records != null)
{
    <table class="table">
        <thead>
            <tr>
                @foreach (var header in Model.Headers)
                {
                    <th>@header</th>
                }
            </tr>
        </thead>
        <tbody>
            @foreach (var record in Model.Records)
            {
                <tr>
                    @foreach (var field in record)
                    {
                        <td>@field</td>
                    }
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>Please select a dataset to analyze.</p>
}
